class Solution {

    // Note that the size of the array is n-1
    int missingNumber(int n, int arr[]) {

        // Your Code Here
        long sum = 0;
        for(int i=0; i<arr.length; i++){
            sum += arr[i];
        }
        long actualSum = (n*(n+1))/2;
        return (int)(actualSum - sum);
    }
}
